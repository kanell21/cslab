#
# Makefile for Heaps

CC=gcc

default: dijkstra_minheap dijkstra_minheap_timers

dijkstra_minheap.o: dijkstra_minheap.c
	$(CC) -Wall -O3 -std=c99  -c dijkstra_minheap.c

dijkstra_minheap: dijkstra_minheap.o
	$(CC) -O3 -o dijkstra_minheap  dijkstra_minheap.o 

dijkstra_minheap_timers: dijkstra_minheap_timers.c
	$(CC) -Wall -DTOTAL -std=c99 -O3 -o dijkstra_minheap_timers  dijkstra_minheap_timers.c

clean: 
	rm dijkstra_minheap *.o
	
